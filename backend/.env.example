# Application
APP_NAME=Tel Call Backend
DEBUG=false

# Security
BOT_TOKEN=replace_me
BOT_USERNAME=replace_me
SECRET_KEY=replace_me
ACCESS_TOKEN_EXPIRE_MINUTES=15

# WebRTC (comma-separated)
STUN_SERVERS=stun:stun.l.google.com:19302
TURN_SERVERS=turns:turn.example.com:5349?transport=tcp
# Frontend origin(s) allowed for CORS (comma-separated). Use * to allow all.
CORS_ALLOW_ORIGINS=https://your-frontend.example.com

# Database
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/tel_call
